{
  "title": "Mission Control and Desktop Switching (C-w/n/p)",
  "rules": [
    {
      "description": "C-w to Mission Control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
              {
                  "type": "frontmost_application_unless",
                  "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                  ],
                  "file_paths": [
                      "/Users/ftherb/build_apps/alacritty/target/release/alacritty"
                  ]
              }
          ]
        }
      ]
    },
    {
      "description": "C-n,C-p to siwtch desktop",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
              {
                  "type": "frontmost_application_unless",
                  "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                  ],
                  "file_paths": [
                      "/Users/ftherb/build_apps/alacritty/target/release/alacritty"
                  ]
              }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
              {
                  "type": "frontmost_application_unless",
                  "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                  ],
                  "file_paths": [
                      "/Users/ftherb/build_apps/alacritty/target/release/alacritty"
                  ]
              }
          ]
        }
      ]
    }
  ]
}
